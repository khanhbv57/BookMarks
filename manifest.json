{
    "manifest_version": 2,
    "name": "Bookmark Extension Example",
    "version": "1.0",
    "chrome_url_overrides":{
    	"newtab": "newtab.html"
  	},

  	"background": {
        "scripts": ["/js/evt.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
    
}